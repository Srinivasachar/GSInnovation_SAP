<mvc:View controllerName="gs.com.GSInnovation.controller.GuidedAssistant" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" xmlns:ce="sap.ui.codeeditor" 
xmlns:mc="sap.suite.ui.microchart" xmlns:common="sap.suite.ui.commons">
        <Page id="idGuidedAssistant" title="ACR Guided Assistant" class="sapUiSizeCompact">
        <content>
        <ObjectHeader
				responsive="true"
				title="Testing">
				<headerContainer>
					<HeaderContainer
						scrollStep="200"
						scrollTime="500"
						showDividers="true"
						class="sapUiSmallMargin">
						<FlexBox
							width="12rem"
							height="10rem"
							alignItems="Center"
							justifyContent="Center"
							direction="Column">
							<Title
								text="Completion Percentage"
								class="sapUiSmallMargin"/>
							<FlexBox width="6rem" height="6rem">
								<mc:RadialMicroChart percentage="10"/>
							</FlexBox>
						</FlexBox>
					</HeaderContainer>
				</headerContainer>
			</ObjectHeader>
            <Panel headerText="{i18n>processFlowProcessTitle}">
            <common:ProcessFlow
                id="processflow1"
                scrollable="false"
                nodes="{process>/Nodes}"
                lanes="{process>/Lanes}"
                nodePress=".onNodePressed">
                <common:nodes>
                    <common:ProcessFlowNode
                        laneId="{process>lane}"
                        nodeId="{process>id}"
                        title="{process>title}"
                        titleAbbreviation="{process>titleAbbreviation}"
                        isTitleClickable="{process>isTitleClickable}"
                        children="{process>children}"
                        state="{process>state}"
                        stateText="{process>stateText}"
                        texts="{process>texts}"
                        highlighted="{process>highlighted}"
                        focused="{process>focused}" />
                </common:nodes>
                <common:lanes>
                    <common:ProcessFlowLaneHeader
                        laneId="{process>id}"
                        iconSrc="{process>icon}"
                        text="{process>label}"
                        position="{process>position}" />
                </common:lanes>
            </common:ProcessFlow>
        </Panel>
            <VBox items="{Suggesstion>/}">
                <Panel expandable="true" expand="onExpand" expanded="{Suggesstion>Expanded}">
                    <headerToolbar>
                        <OverflowToolbar active="true" press="onOverflowToolbarPress">
                            <Title text="{Suggesstion>Header}" />
                            <ToolbarSpacer />
                            <Button text="Reset"/>
                            <Button icon="{= ${Suggesstion>Completed}? 'sap-icon://accept' : ''}" type="{= ${Suggesstion>Completed} ? 'Success' : 'Default' }"/>
                        </OverflowToolbar>
		            </headerToolbar>
                    <content>
			            <OverflowToolbar content="{Suggesstion>Childrens}">
                            <ToggleButton text="{Suggesstion>Name}"/>
                        </OverflowToolbar>
		            </content>
                </Panel>
            </VBox>
         </content>
        <footer>
            <OverflowToolbar id="OverflowToolbar">	
                <ToolbarSpacer id="ToolbarSpacer3"/>
                <Button press="onActivateConfigParamPress" text="Proceed Creation" type="Emphasized"/>
                <Button press="onCancelConfigParamPress" text="Discard"/>
            </OverflowToolbar>
        </footer>
    </Page>
</mvc:View>